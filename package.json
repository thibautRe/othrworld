{
  "name": "@othrwolrd/monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "scripts": {
    "prestart": "yarn workspace @othrworld/namegen-core build && yarn workspace @othrworld/namegen-nobel build",
    "start": "concurrently yarn:start:*",
    "start:shared": "concurrently yarn:start:shared:*",
    "start:shared:namegen-core": "yarn workspace @othrworld/namegen-core start",
    "start:shared:namegen-nobel": "yarn workspace @othrworld/namegen-nobel start"
  }
}
