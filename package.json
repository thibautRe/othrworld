{
  "name": "@othrworld/monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "scripts": {
    "prestart": "yarn workspace @othrworld/core build && yarn workspace @othrworld/gen-core build && yarn workspace @othrworld/namegen-nobel build && yarn workspace @othrworld/systemgen build",
    "start": "concurrently yarn:start:*",
    "start:shared": "concurrently yarn:start:shared:*",
    "start:shared:core": "yarn workspace @othrworld/core start",
    "start:shared:gen-core": "yarn workspace @othrworld/gen-core start",
    "start:shared:namegen-nobel": "yarn workspace @othrworld/namegen-nobel start",
    "start:shared:systemgen": "yarn workspace @othrworld/systemgen start"
  }
}
